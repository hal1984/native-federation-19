{
  "name": "native-federation-19",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:all": "concurrently \"npm run start:mfe1\" \"npm run start:shell\"",
    "start:mfe1": "ng serve mfe1",
    "start:shell": "ng serve shell",
    "build": "concurrently \"npm run build:mfe1\" \"npm run build:shell\"",
    "build:mfe1": "ng build mfe1",
    "build:shell": "ng build shell",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:mfe1": "node dist/mfe1/server/server.mjs",
    "serve:ssr:shell": "node dist/shell/server/server.mjs",
    "serve:ssr": "concurrently \"npm run serve:ssr:mfe1\" \"npm run serve:ssr:shell\"",
    "start": "npm run build:mfe1 && npm run serve:ssr:mfe1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.3",
    "@angular/common": "^19.1.3",
    "@angular/compiler": "^19.1.3",
    "@angular/core": "^19.1.3",
    "@angular/forms": "^19.1.3",
    "@angular/platform-browser": "^19.1.3",
    "@angular/platform-browser-dynamic": "^19.1.3",
    "@angular/platform-server": "^19.1.3",
    "@angular/router": "^19.1.3",
    "@angular/ssr": "^19.1.4",
    "@softarc/native-federation-node": "^2.0.20",
    "cors": "^2.8.5",
    "es-module-shims": "^2.0.5",
    "express": "^4.21.2",
    "rxjs": "~7.8.1",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-architects/native-federation": "^19.0.7",
    "@angular-devkit/build-angular": "^19.1.4",
    "@angular/cli": "^19.1.4",
    "@angular/compiler-cli": "^19.1.3",
    "@types/express": "^5.0.0",
    "@types/jasmine": "~5.1.5",
    "@types/node": "^22.10.10",
    "concurrently": "^9.1.2",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.3"
  }
